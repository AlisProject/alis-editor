  "scripts": {
    "serve": "vue-cli-service serve src/develop.js",
    "build": "vue-cli-service build --target lib --name AlisEditor src/index.js",
    "lint": "vue-cli-service lint",
    "format": "prettier-eslint --write \"./**/*.{js,vue}\" --ignore \"./node_modules/**\" ",
